	<run spec="MCMC" chainLength="200000000" storeEvery="10000">

		<state>
		<stateNode id="seg1.tree" spec="beast.app.mascot.beauti.TreeWithTrait">
			<typeTrait idref="typeTrait"/>
			<trait idref="traitSet"/>
			<taxonset idref="taxonSet"/>
		</stateNode>
        <stateNode id="popSize" spec="RealParameter" value="5.2046 1.3092"/>
        <stateNode id="migrationRate" spec="RealParameter" dimension="2" value="0.58303 0.92834"/>
        <stateNode id="clockRate.high" spec="RealParameter" value="0.005"/>
        <stateNode id="clockRate.low" spec="RealParameter" value="0.0005"/>
		</state>
	
    		<init id="RandomTree" spec="beast.evolution.tree.RandomTree" estimate="false" initial="@seg1.tree" taxa="@seg1align">
        		<populationModel id="ConstantPopulation0" spec="ConstantPopulation">
            			<parameter id="randomPopSize" name="popSize">5.2046 1.3092</parameter>
        		</populationModel>
    		</init>

		<distribution id="posterior" spec="CompoundDistribution">
            <distribution id="likelihood" spec="CompoundDistribution">
                <distribution spec="TreeLikelihood">
                    <data idref="seg1align"/>
                    <tree idref="seg1.tree"/>
                    <siteModel spec="SiteModel" mutationRate="@clockRate.low">
                        <substModel spec="JukesCantor"/>
                    </siteModel>
        			</distribution>
			</distribution>

		<distribution id="prior" spec="util.CompoundDistribution">
			<distribution id="structuredCoalescent" spec="beast.mascot.distribution.Mascot">
                	<dynamics id="constant" spec="beast.mascot.dynamics.Constant" Ne="@popSize" backwardsMigration="@migrationRate" dimension="0" typeTrait="@typeTrait"/>
	                <structuredTreeIntervals id="TreeIntervals" spec="beast.mascot.distribution.StructuredTreeIntervals" tree="@seg1.tree"/>
	        </distribution>

			<distribution id="parametersPrior" spec="CompoundDistribution">
                    <distribution spec="beast.math.distributions.Prior" x="@migrationRate">
                        <distr spec="beast.math.distributions.LogNormalDistributionModel" M="-1.7344" S="0.5"/>
                		</distribution>

                		<distribution spec="beast.math.distributions.Prior" x="@popSize">
                        <distr spec="beast.math.distributions.LogNormalDistributionModel" M="0.5681" S="0.5"/>
                    </distribution>
            </distribution> 
 		</distribution>
	</distribution>

		<operator id="MigratioOperator" spec="ScaleOperator"
                  weight="5.0" scaleFactor="0.8"  optimise="false"  parameter="@migrationRate">
        </operator>

        <operator id="PopSizeScaler" spec="ScaleOperator"
                  weight="5.0"  scaleFactor="0.8" optimise="false" parameter="@popSize">
        </operator>


	<operator id="TreeScaler" spec="ScaleOperator" tree="@seg1.tree" weight="3.0"/>

    <operator id="RootScaler" spec="ScaleOperator" tree="@seg1.tree" weight="1.0"/>

    <operator id="Uniform" spec="Uniform" tree="@seg1.tree" weight="30.0"/>

    <operator id="SubtreeSlide" spec="SubtreeSlide" tree="@seg1.tree" weight="15.0"/>

    <operator id="Narrow" spec="Exchange" tree="@seg1.tree" weight="15.0"/>

    <operator id="WilsonBalding" spec="WilsonBalding" tree="@seg1.tree" weight="3.0"/>



	<logger logEvery="10000" fileName="$(filebase).log">
        <log idref="posterior"/>
        <log idref="likelihood"/>
        <log idref="structuredCoalescent"/>
        <log id="treeHeight" spec="beast.evolution.tree.TreeHeightLogger" tree="@seg1.tree"/>
        <log idref="constant"/>
		<log spec="TreeStatLogger" tree="@seg1.tree"/>
	</logger>

	<logger id="screenlog" logEvery="100000">
		<log idref="posterior"/>
        <log idref="likelihood"/>
        <log spec="ESS" arg="@posterior"/>
	</logger>

    <logger id="treelog" fileName="$(filebase).seg1.trees" logEvery="1000000" mode="tree">
        <log id="structuredTreelog" spec="beast.mascot.logger.StructuredTreeLogger" mascot="@structuredCoalescent"/>
    </logger>

    </run>
