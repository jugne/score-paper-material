    	<operator id="Narrow.t4" spec="Exchange" tree="@seg4.tree" weight="15.0"/>
    	<operator id="WilsonBalding.t4" spec="WilsonBalding" tree="@seg4.tree" weight="3.0"/>



	<logger logEvery="10000" fileName="$(filebase).log">
        <log idref="posterior"/>
        <log idref="likelihood"/>
        <log idref="structuredCoalescent.t1"/>
        <log idref="structuredCoalescent.t2"/>
        <log idref="structuredCoalescent.t3"/>
        <log idref="structuredCoalescent.t4"/>
        <log idref="constant.t1"/>
	<log spec="TreeStatLogger" tree="@seg1.tree"/>
	<log spec="TreeStatLogger" tree="@seg2.tree"/>
	<log spec="TreeStatLogger" tree="@seg3.tree"/>
	<log spec="TreeStatLogger" tree="@seg4.tree"/>
	</logger>

	<logger id="screenlog" logEvery="100000">
		<log idref="posterior"/>
        <log idref="likelihood"/>
        <log spec="ESS" arg="@posterior"/>
	</logger>

    <logger id="treelog1" fileName="$(filebase).seg1.trees" logEvery="1000000" mode="tree">
        <log id="structuredTreelog.t1" spec="beast.mascot.logger.StructuredTreeLogger" mascot="@structuredCoalescent.t1"/>
    </logger>
    <logger id="treelog2" fileName="$(filebase).seg2.trees" logEvery="1000000" mode="tree">
        <log id="structuredTreelog.t2" spec="beast.mascot.logger.StructuredTreeLogger" mascot="@structuredCoalescent.t2"/>
    </logger>
    <logger id="treelog3" fileName="$(filebase).seg3.trees" logEvery="1000000" mode="tree">
