        				</populationModel>
    		</init>
		<init id="RandomTree4" spec="beast.evolution.tree.RandomTree" estimate="false" initial="@seg4.tree" taxa="@seg4align">
        				<populationModel id="ConstantPopulation4" spec="ConstantPopulation">
            			<parameter id="randomPopSize4" name="popSize">0.92345 0.1273</parameter>
        				</populationModel>
    		</init>
		

	<distribution id="posterior" spec="CompoundDistribution">
        		<distribution id="likelihood" spec="CompoundDistribution">
            		<distribution spec="TreeLikelihood">
                    		<data idref="seg1align"/>
                    		<tree idref="seg1.tree"/>
                    		<siteModel spec="SiteModel" mutationRate="@clockRate.high">
                        			<substModel spec="JukesCantor"/>
                    		</siteModel>
        			</distribution>
			 <distribution spec="TreeLikelihood">
                    		<data idref="seg2align"/>
                    		<tree idref="seg2.tree"/>
